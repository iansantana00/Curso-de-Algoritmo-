algoritmo "Desafio5"
var
 N1: Inteiro
inicio
 Escreva("Digite uma distancia em metros ")
 Leia (N1)
 Escreval("A distancia de ", N1,"m", " corresponde a: " )
 Escreval(N1/1000,"km")
 Escreval(N1/100,"hm")
 Escreval(N1/10,"dam")
 Escreval(N1*10,"dm")
 Escreval(N1*100,"cm")
 Escreval(N1*1000,"mm")
fimalgoritmo
