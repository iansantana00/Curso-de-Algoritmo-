algoritmo "ContarAsuaEscolha "
var
   valor, contador: Inteiro

inicio
      contador <- 0
      Escreva ("Quer contar ate quanto? ")
      Leia (valor)
      Enquanto (contador <= valor ) faca
          Escreval (contador)
          contador <- contador + 1
      FimEnquanto
      Escreval ("Terminei de contar")
      
fimalgoritmo
